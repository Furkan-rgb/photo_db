from '@material-ui/icons'\n\nconst useStyles = makeStyles((theme) => ({\n  container: {\n    minHeight: '100vh',\n    width: '100%',\n    maxWidth: 1920,\n  },\n  mainContainer: {\n    width: '100%',\n  },\n  bottomNav: {\n    width: '100%',\n    top: 'auto',\n    bottom: 0,\n    position: 'fixed',\n  },\n  fabButton: {\n    position: 'absolute',\n    zIndex: 1,\n    top: -30,\n    left: 0,\n    right: 0,\n    margin: '0 auto',\n  },\n  input: {\n    display: 'none',\n  }\n}));\n\nfunction App() {\n  const classes = useStyles()\n  const [error, setError] = useState(\"\");\n  const alert = useAlert();\n  //Store image hook\n  const [file, setFile] = useState(null);\n\n  //Allowed filetypes\n  const types = ['image/png', 'image/jpeg'];\n\n  //Image handler\n  //selected image\n  const changeHandler = (e) => {\n    let selected = e.target.files[0];\n    console.log(selected)\n    //Check if a file is selected\n    if (selected && types.includes(selected.type)) {\n      //store the file\n      setFile(selected);\n      setError('');\n      alert.success(\"Image succesfully uploaded\");\n    }\n    else {\n      setFile(null);\n      setError('Please select an image file (png or jpeg)')\n    }\n  }\n  // Bottom  nav icons\n  const [value, setValue] = React.useState(0);\n\n  return (\n    <Grid container className={classes.mainContainer} justify=\"center\">\n\n      {/* Routing */}\n      <Grid container item justify=\"center\" className={classes.container} align='center'>\n        <Router basename=\"/storygram\">\n          <AuthProvider>\n            <Switch>\n              <PrivateRoute exact path=\"/\" component={Dashboard} />\n              <PrivateRoute exact path=\"/UpdateProfile\" component={UpdateProfile} />\n              <Route path=\"/signup\" component={Signup} />\n              <Route path=\"/login\" component={Login} />\n              <Route path=\"/forgot-password\" component={ForgotPassword} />\n            </Switch>\n\n\n            {/* Mobile app bar */}\n            <Hidden smUp>\n              <BottomNavigation\n                value={value}\n                onChange={(event, newValue) => {\n                  setValue(newValue);\n                }}\n                showLabels\n                className={classes.bottomNav}\n              >\n                <BottomNavigationAction\n                  label=\"Feed\"\n                  icon={<Home />}\n                  component={Link} to=\"/\"\n                />\n                <div>\n                  {/* Upload image FAB */}\n                  <input\n                    accept=\"image/*\"\n                    id=\"fab-button-file\"\n                    className={classes.input}\n                    type=\"file\"\n                    onChange={changeHandler}\n                  />\n                  <label htmlFor=\"fab-button-file\">\n                    <Fab color=\"secondary\" aria-label=\"add\" className={classes.fabButton} component=\"span\">\n                      <Add />\n                    </Fab>\n                  </label>\n                </div>\n\n                <BottomNavigationAction\n                  label=\"Account\"\n                  icon={<AccountBox />}\n                  component={Link} to=\"/UpdateProfile\"\n                />\n              </BottomNavigation>\n            </Hidden>\n          </AuthProvider>\n        </Router>\n      </Grid>\n    </Grid>\n  )\n}\n\nexport default App;\n","/Users/furkan/Documents/AD-S3/Passion Project/Photography DB.nosync/photo_db/src/components/Signup.js",[],"/Users/furkan/Documents/AD-S3/Passion Project/Photography DB.nosync/photo_db/src/contexts/AuthContext.js",[],"/Users/furkan/Documents/AD-S3/Passion Project/Photography DB.nosync/photo_db/src/firebase.js",[],"/Users/furkan/Documents/AD-S3/Passion Project/Photography DB.nosync/photo_db/src/components/Dashboard.js",["74","75"],"/Users/furkan/Documents/AD-S3/Passion Project/Photography DB.nosync/photo_db/src/components/Login.js",[],"/Users/furkan/Doc